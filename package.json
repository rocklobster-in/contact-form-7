{
	"name": "contact-form-7",
	"version": "6.1.2",
	"description": "JavaScript packages for Contact Form 7.",
	"private": true,
	"scripts": {
		"build:all": "npm run build:front && npm run build:block && npm run build:swv && npm run build:recaptcha && npm run build:stripe && npm run build:admin",
		"start:front": "wp-scripts start includes/js/src/index.js --output-path=includes/js",
		"build:front": "wp-scripts build includes/js/src/index.js --output-path=includes/js",
		"start:block": "wp-scripts start includes/block-editor/src/index.js --output-path=includes/block-editor",
		"build:block": "wp-scripts build includes/block-editor/src/index.js --output-path=includes/block-editor",
		"start:swv": "wp-scripts start includes/swv/js/src/index.js --output-path=includes/swv/js",
		"build:swv": "wp-scripts build includes/swv/js/src/index.js --output-path=includes/swv/js",
		"start:recaptcha": "wp-scripts start modules/recaptcha/src/index.js --output-path=modules/recaptcha",
		"build:recaptcha": "wp-scripts build modules/recaptcha/src/index.js --output-path=modules/recaptcha",
		"start:stripe": "wp-scripts start modules/stripe/src/index.js --output-path=modules/stripe",
		"build:stripe": "wp-scripts build modules/stripe/src/index.js --output-path=modules/stripe",
		"start:admin": "wp-scripts start admin/includes/js/src/index.js --output-path=admin/includes/js",
		"build:admin": "wp-scripts build admin/includes/js/src/index.js --output-path=admin/includes/js",
		"packages-update": "wp-scripts packages-update"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rocklobster-in/contact-form-7.git"
	},
	"author": "takayukister",
	"license": "GPL-2.0-only",
	"bugs": {
		"url": "https://github.com/rocklobster-in/contact-form-7/issues"
	},
	"homepage": "https://github.com/rocklobster-in/contact-form-7#readme",
	"devDependencies": {
		"@contactable/swv": "^1.4.1",
		"@wordpress/scripts": "^30.24.0"
	}
}
